  "scripts": {
    "start": "concurrently \"npm run watch-css\" \"set HTTPS=true&&react-scripts-ts start\"",
    "build": "react-scripts-ts build",
    "test:all": "jest --env=jsdom",
    "test:coverage": "jest --env=jsdom  --coverage --collectCoverageFrom=src/**/*.tsx --coverageDirectory=output/coverage/jest",
    "test:e2e": "mocha e2e_tests --no-timeouts --require babel-core/register --require babel-polyfill",
    "lint": "eslint \"src/**/*.ts?\"",
    "lint-report": "eslint --format junit -o reports/js-lint-results.xml \"src/**/*.ts?\"",
    "lint-fix": "npm run lint -- --fix",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
